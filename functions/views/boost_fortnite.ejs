<%- include('header') %>


<!-- ##### Post Details Area Start ##### -->
<section id="mainSection" class="post-news-area section-padding-0-0">
    <div class="container">
        <div class="row justify-content-center mt-30 mb-30">
            <!-- Post Details Content Area -->
            <div class="col-lg-8">
                <div class="">
                    <div class="post-details-content">
                        <!-- Start Boost Form -->
                        <div class="container boostContainer">
                            <div class='container checkout hidden'>
                                <div class="checkout-content">
                                    <div>
                                        <button type="button" class="close exit-button" onclick="hideCheckout()" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <h2>Choose Your BOOSTER</h2>
                                    <div class="container-fluid">
                                        <div id="myCarousel" class="carousel slide" data-ride="carousel">
                                            <div class="carousel-inner row w-100 mx-auto">
                                                <div class="carousel-item col-md-4 active">
                                                    <div class="card">
                                                        <img class="card-img-top" src="./img/bg-img/header_fortnite.png"
                                                            alt="Card image cap">
                                                        <div class="card-body">
                                                            <h4 class="card-title">temp1</h4>
                                                            <p class="card-text">
                                                                test2
                                                            </p>
                                                            <button name="booster_choose_btn" option="fortnite_booster_1" class="btn btn-primary">Choose</button>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="carousel-item col-md-4">
                                                    <div class="card">
                                                        <img class="card-img-top" src="./img/bg-img/header_fortnite.png"
                                                            alt="Card image cap">
                                                        <div class="card-body">
                                                            <h4 class="card-title">temp2</h4>
                                                            <p class="card-text">
                                                                test
                                                            </p>
                                                            <button name="booster_choose_btn" option="fortnite_booster_2" class="btn btn-primary">Choose</button>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="carousel-item col-md-4">
                                                    <div class="card">
                                                        <img class="card-img-top" src="./img/bg-img/header_fortnite.png"
                                                            alt="Card image cap">
                                                        <div class="card-body">
                                                            <h4 class="card-title">temp3</h4>
                                                            <p class="card-text">
                                                                test3
                                                            </p>
                                                            <button name="booster_choose_btn" option="fortnite_booster_3" class="btn btn-primary">Choose</button>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="carousel-item col-md-4">
                                                    <div class="card">
                                                        <img class="card-img-top" src="./img/bg-img/header_fortnite.png"
                                                            alt="Card image cap">
                                                        <div class="card-body">
                                                            <h4 class="card-title">temp4</h4>
                                                            <p class="card-text">
                                                                test4
                                                            </p>
                                                            <button name="booster_choose_btn" option="fortnite_booster_4" class="btn btn-primary">Choose</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="container">
                                                <div class="row">
                                                    <div class="col-12 text-center mt-4">
                                                        <a class="btn btn-outline-secondary slider-colors mx-1 prev"
                                                            href="javascript:void(0)" title="Previous">
                                                            <i class="fa fa-lg fa-chevron-left"></i>
                                                        </a>
                                                        <a class="btn btn-outline-secondary slider-colors mx-1 next"
                                                            href="javascript:void(0)" title="Next">
                                                            <i class="fa fa-lg fa-chevron-right"></i>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <button onclick="continueCheckout();" class="btn checkout-button">Continue</button>
                                </div>

                            </div>
                            <div class="boost_form fortnite_colors">
                                <div class="justify-content-center">
                                    <img src="img/bg-img/header_fortnite.png" width="100%" height="50%" />
                                </div>

                                <div class="boost_nav_bar_img"
                                    style="background-image:url('img/bg-img/nav_fortnite.png');">
                                    <div class="col-12">
                                        <ul class="nav nav-tabs " id="myTab" role="tablist">
                                            <li class="nav-item">
                                                <a class="nav-link active show" id="challenges-tab" data-toggle="tab"
                                                    href="#challenges" role="tab" aria-controls="challenges"
                                                    aria-selected="true">Challenges</a>
                                            </li>

                                            <li class="nav-item">
                                                <a class="nav-link" id="wins-tab" data-toggle="tab" href="#wins"
                                                    role="tab" aria-controls="wins" aria-selected="false">Wins</a>
                                            </li>

                                            <li class="nav-item">
                                                <a class="nav-link" id="arena-tab" data-toggle="tab" href="#arena"
                                                    role="tab" aria-controls="arena" aria-selected="false">Arena</a>
                                            </li>

                                        </ul>
                                    </div>
                                </div>

                                <!-- Content -->
                                <div class="content boost_content_img"
                                    style="background-image:url('img/bg-img/fortnite_boost_wallpaper.png');">
                                    <div class="form_content  d-flex flex-column justify-content-center ">
                                        <form action="" onsubmit="return false;" class="tab-content">
                                            <!-- challenges -->
                                            <div class="tab-pane fade show active challenges" id="challenges"
                                                role="tabpanel" aria-labelledby="challenges-tab">
                                                <div class="p-2">
                                                    <div class="form-text">
                                                        Platform
                                                    </div>
                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <select class="form-select">
                                                            <option>PS4</option>
                                                            <option>Xbox</option>
                                                            <option>PC</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="p-2">
                                                    <div class="form-text">
                                                        Number of Challenges
                                                    </div>

                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <input id="fort_challenges" class="custom_number_slider"
                                                            type="number" value="1" min="1" max="100" step="1" />
                                                    </div>

                                                </div>
                                            </div>

                                            <!-- Wins -->
                                            <div class="tab-pane fade" id="wins" role="tabpanel"
                                                aria-labelledby="wins-tab">
                                                <div class="p-2">
                                                    <div class="form-text">
                                                        Platform
                                                    </div>
                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <select class="form-select">
                                                            <option>PS4</option>
                                                            <option>Xbox</option>
                                                            <option>PC</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="p-2">
                                                    <div class="form-text">
                                                        Queue Type
                                                    </div>
                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <select class="form-select">
                                                            <option>Solo</option>
                                                            <option>duo</option>
                                                            <option>Squad</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="p-2">
                                                    <div class="form-text">
                                                        Wins
                                                    </div>

                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <input id="fort_wins" class="custom_number_slider" type="number"
                                                            value="1" min="1" max="100" step="1" />
                                                    </div>

                                                </div>
                                            </div>

                                            <!-- Arena -->
                                            <div class="tab-pane fade" id="arena" role="tabpanel"
                                                aria-labelledby="arena-tab">
                                                <div class="p-2">
                                                    <div class="form-text">
                                                        Platform
                                                    </div>
                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <select class="form-select">
                                                            <option>PS4</option>
                                                            <option>Xbox</option>
                                                            <option>PC</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="p-2">
                                                    <div class="form-text">
                                                        Queue Type
                                                    </div>
                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <select class="form-select" id="queue_arena">
                                                            <option value="1">Solo</option>
                                                            <option value="2">duo</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="p-2">
                                                    <div class="form-text" id="c_division">
                                                        Current Division
                                                    </div>

                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <input id="fort_arena_c" class="custom_number_slider"
                                                            type="number" value="1" min="1" max="6" step="1" />
                                                    </div>

                                                </div>

                                                <div class="p-2">
                                                    <div class="form-text" id="#d_division">
                                                        Desired Division
                                                    </div>

                                                    <div class="p-2 col-md-6 mx-auto">
                                                        <input id="fort_arena_d" class="custom_number_slider"
                                                            type="number" value="2" min="2" max="7" step="1" />
                                                    </div>

                                                </div>
                                            </div>

                                            <div class="p-2">
                                                <h2 class="form-text">
                                                    Price
                                                </h2>
                                                <div class="form-price fortnite_price">
                                                    $<span id="price">0.00</span>
                                                </div>
                                            </div>

                                            <div class="p-2 mt-50 col-md-6 mx-auto">
                                                <button class="btn egames-btn w-100" onclick="checkout();">
                                                    Check Out
                                                </button>
                                            </div>

                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- End Boost Form -->

                    </div>
                </div>
            </div>

            <!-- Sidebar Widget -->
            <div class="col">
                <!-- Single Widget Area -->
                <div class="d-flex justify-content-center mt-1">
                    <div class="container">
                        <!-- Start Widgets area -->
                        <%- include('reviews_widget') %>
                        <!-- End Widgets area -->
                    </div>
                </div>
            </div>
        </div>

        <!-- photos section -->
